---
import MaintainersLayout from "@layouts/en/EnMaintainersLayout.astro";
import { type CollectionEntry, getCollection } from "astro:content";

export async function getStaticPaths() {
  const posts = await getCollection("maintainers");
  return (
    posts?.map((post) => ({
      params: { slug: post.slug },
      props: post,
    })) || []
  );
}
type Props = CollectionEntry<"maintainers">;

const post = Astro.props;
const { Content } = await post.render();
---

<MaintainersLayout>
  <Content />
</MaintainersLayout>
